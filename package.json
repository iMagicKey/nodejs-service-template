{
    "name": "projectname",
    "version": "1.0.0",
    "description": "NextJS template for Service (NodeJS)",
    "main": "app/index.js",
    "scripts": {
        "live": "node --import=specifier-resolution-node/register --env-file=.env app",
        "dev": "node --import=specifier-resolution-node/register --env-file=.env.dev app",
        "init": "npm install && node --import=specifier-resolution-node/register system/init",
        "install-service": "node --import=specifier-resolution-node/register system/service install",
        "uninstall-service": "node --import=specifier-resolution-node/register system/service uninstall",
        "create-certs": "node --import=specifier-resolution-node/register system/certs",
        "clear-logs": "node --import=specifier-resolution-node/register system/logs",
        "build": "next build",
        "lint": "next lint"
    },
    "type": "module",
    "dependencies": {
        "@reduxjs/toolkit": "^2.6.0",
        "imagic-utils": "^1.0.1",
        "imagic-web-server": "^1.0.3",
        "next": "^15.2.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-redux": "^9.2.0",
        "redux": "^5.0.1",
        "sharp": "^0.33.5",
        "ws": "^8.18.1"
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "^15.2.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.57.1",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-next": "^15.2.0",
        "eslint-config-prettier": "^10.0.2",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-next": "^0.0.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.2.3",
        "postcss": "^8.5.3",
        "prettier": "^3.5.2",
        "specifier-resolution-node": "^1.1.4",
        "tailwindcss": "^4.0.9"
    },
    "postcss": {
        "plugins": {
            "tailwindcss": {},
            "autoprefixer": {}
        }
    },
    "author": "DoubleFun",
    "license": "MIT",
    "engines": {
        "node": ">=20"
    }
}
