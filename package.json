{
    "name": "projectname",
    "version": "1.0.0",
    "description": "NextJS template for Service (NodeJS)",
    "main": "app/index.js",
    "scripts": {
        "live": "APP_ENV=live NODE_ENV=production node --experimental-specifier-resolution=node app",
        "dev": "APP_ENV=dev NODE_ENV=development node --experimental-specifier-resolution=node app",
        "init": "npm install && APP_ENV=dev NODE_ENV=development node --experimental-specifier-resolution=node system/init",
        "install-service": "node --experimental-specifier-resolution=node system/service install",
        "uninstall-service": "node --experimental-specifier-resolution=node system/service uninstall",
        "create-certs": "node --experimental-specifier-resolution=node system/certs",
        "clear-logs": "node --experimental-specifier-resolution=node system/logs",
        "build": "next build",
        "lint": "next lint"
    },
    "type": "module",
    "dependencies": {
        "imagic-web-server": "^1.0.0",
        "next": "14.0.1",
        "react": "^18",
        "react-dom": "^18",
        "react-redux": "^8.1.3",
        "redux": "^4.2.1",
        "sharp": "^0.32.6"
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "^14.0.1",
        "autoprefixer": "^10.0.1",
        "eslint": "^8.38.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-next": "^14.0.1",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-next": "^0.0.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.0.1",
        "postcss": "^8",
        "prettier": "3.0.3",
        "tailwindcss": "^3.4.0"
    },
    "postcss": {
        "plugins": {
            "tailwindcss": {},
            "autoprefixer": {}
        }
    },
    "author": "DoubleFun",
    "license": "MIT"
}
